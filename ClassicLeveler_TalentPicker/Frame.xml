<Ui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.blizzard.com/wow/ui/">
	<Script file="Frame.lua" />
	<Frame name="Frame1" parent="UIParent" toplevel="true" movable="true" enableMouse="true" clampedToScreen="true">
		<Size x="166" y="104" />
		<Anchors>
			<Anchor point="CENTER" />
		</Anchors>
		<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
			<BackgroundInsets>
				<AbsInset left="11" right="12" top="12" bottom="11" />
			</BackgroundInsets>
			<TileSize>
				<AbsValue val="32" />
			</TileSize>
			<EdgeSize>
				<AbsValue val="32" />
			</EdgeSize>
		</Backdrop>
		<Frames>
			<Button inherits="UIPanelButtonTemplate" text="Train" name="TrainButton">
				<Size x="118" y="53" />
				<Anchors>
					<Anchor point="CENTER" />
				</Anchors>
				<Scripts>
					<OnClick>
						Button1_OnClick()
					</OnClick>
				</Scripts>
			</Button>
			<Button inherits="UIPanelCloseButton" text="Button2">
				<Size x="25" y="25" />
				<Anchors>
					<Anchor point="TOPRIGHT" />
				</Anchors>
			</Button>
		</Frames>
		<Scripts>
			<OnLoad>
				Frame1_OnLoad();
			</OnLoad>
			<OnEvent>
				Frame1_OnEvent(event);
			</OnEvent>
			<OnDragStop>
				Frame1:StopMovingOrSizing();
			</OnDragStop>
			<OnDragStart>
				Frame1:StartMoving();
			</OnDragStart>
		</Scripts>
	</Frame>
</Ui>